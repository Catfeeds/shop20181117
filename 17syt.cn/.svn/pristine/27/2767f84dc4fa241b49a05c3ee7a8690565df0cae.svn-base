html {
	overflow: hidden;
}
body {
	width: 100%;
	height: 100%;
	font-family: "微软雅黑", Arial;
}
header, section {
	width: 100%;
}
table {
	border-collapse: collapse;
}
a:focus {
	outline: none;
}
header {
	position: fixed;
	height: 74px;
	color: #ccc;
	z-index: 999;
	height: 85px;
}
.header{
	background:#fff;
}
header .header-body{
	width:1045px;
	margin:0 auto;
}
header .logo {
	width: 320px;
}
header .logo img,header .logo .words{
	float:left;

}
header .logo img{
	margin-top:35px;
}
header .logo .words{
	line-height:112px;
	margin-left:40px;
}

header .words {
	letter-spacing: 1px;
	font-weight: normal;

}
header .logo ,header nav.menu{
	float:left;
}
header nav.menu {
	text-align: center;
	-webkit-transition: all .2s ease-in-out 0s;
	-moz-transition: all .2s ease-in-out 0s;
	-ms-transition: all .2s ease-in-out 0s;
	-o-transition: all .2s ease-in-out 0s;
	transition: all .2s ease-in-out 0s;
}
header nav.menu ul.list-inline{
	margin:0;

}
header nav.menu ul li {
	font-size: 14px;
	padding:0;
	display:block;
	float:left;
	-webkit-transition: all .2s ease-in-out 0s;
	-moz-transition: all .2s ease-in-out 0s;
	-ms-transition: all .2s ease-in-out 0s;
	-o-transition: all .2s ease-in-out 0s;
	transition: all .2s ease-in-out 0s;
}

header nav.menu ul li a {
	display:inline-block;
	height:85px;
	padding:0 9px;
	line-height:111px;
	color: #1a1a1a;
	cursor: pointer;
}
header nav.menu ul li.call-me{
	margin-left:15px;
}
header nav.menu ul li.call-me a{
	display: inline-block;
	height: 38px;
	width: 110px;
	border: 1px solid #ff9900;
	border-radius: 20px;
	color: #ff9900;
	margin-top: 35px;
	line-height: 38px;
}
header nav.menu ul li a:hover, header nav.menu ul li.active a {
	background:#003366;
	text-decoration: none;
	color:#fff;
}
header nav.menu ul li.call-me a:hover{
	background:#ff9900;
	color:#fff;
}
header .hotline {
	position: fixed;
	top: 32px;
	right: 215px;
	-webkit-transition: all .2s ease-in-out 0s;
	-moz-transition: all .2s ease-in-out 0s;
	-ms-transition: all .2s ease-in-out 0s;
	-o-transition: all .2s ease-in-out 0s;
	transition: all .2s ease-in-out 0s;
}
header.fixed .hotline {
	top: 20px;
	right: 185px;
}
header .hotline span {
	position: fixed;
	color: #00dfb9;
	font-family: Arial;
	font-weight: bold;
	font-size: 26px;
	letter-spacing: 1px;
	-webkit-transition: all .2s ease-in-out 0s;
	-moz-transition: all .2s ease-in-out 0s;
	-ms-transition: all .2s ease-in-out 0s;
	-o-transition: all .2s ease-in-out 0s;
	transition: all .2s ease-in-out 0s;
}
header.fixed .hotline span {
	font-size: 20px;
}
header .hotline u {
	position: fixed;
	top: 35px;
	right: 230px;
	display: block;
	width: 30px;
	height: 30px;
	text-decoration: none;
	background: url(../Images/icons.png) no-repeat;
	-webkit-transition: all .2s ease-in-out 0s;
	-moz-transition: all .2s ease-in-out 0s;
	-ms-transition: all .2s ease-in-out 0s;
	-o-transition: all .2s ease-in-out 0s;
	transition: all .2s ease-in-out 0s;
}
header.fixed .hotline u {
	top: 20px;
	right: 195px;
}
header .hotline a {
	color: #00dfb9;
}
header .menu-icon {
	position: fixed;
	top: 34px;
	right: 48px;
	font-size: 24px;
	color: #00dfb9;
	z-index: 99;
	display: none;
}
header.fixed .menu-icon {
	top: 18px;
}
header .menu-icon a {
	color: #00dfb9;
	text-decoration: none;
}
header .menu-icon span {
	margin-left: 15px;
	-moz-opacity: 0.5;
	opacity: 0.5;
	cursor: pointer;
	-webkit-transition: all .3s ease-in-out 0s;
	-moz-transition: all .3s ease-in-out 0s;
	-ms-transition: all .3s ease-in-out 0s;
	-o-transition: all .3s ease-in-out 0s;
	transition: all .3s ease-in-out 0s;
}
header .menu-icon span.glyphicon-earphone {
	display: none;
}
header .menu-icon span:hover, header .menu-icon span.active {
	-moz-opacity: 1;
	opacity: 1;
	-webkit-transition: all .3s ease-in-out 0s;
	-moz-transition: all .3s ease-in-out 0s;
	-ms-transition: all .3s ease-in-out 0s;
	-o-transition: all .3s ease-in-out 0s;
	transition: all .3s ease-in-out 0s;
}
header .bg {
	position: absolute;
	top: 0px;
	width: 100%;
	height: 0px;
	background: #fff;
	-moz-opacity: 0.6;
	opacity: 0.6;
	z-index: -1;
	-webkit-transition: all .2s ease-in-out 0s;
	-moz-transition: all .2s ease-in-out 0s;
	-ms-transition: all .2s ease-in-out 0s;
	-o-transition: all .2s ease-in-out 0s;
	transition: all .2s ease-in-out 0s;
}
header.fixed .bg {
	height: 70px;
}
div.welcome {
	position: fixed;
	width: 100%;
	height: 100%;
	background: url(../Images/welcome.jpg) 30% 80%;
	background-color: White;
	-moz-background-size: cover;
	background-size: cover;
	-webkit-transition: all .3s ease-in-out .5s;
	-moz-transition: all .3s ease-in-out .5s;
	-ms-transition: all .3s ease-in-out .5s;
	-o-transition: all .3s ease-in-out .5s;
	transition: all .3s ease-in-out .5s;
	z-index: 999;
}
div.welcome p {
	display: block;
