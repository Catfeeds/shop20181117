    function autoPlay(){
        var i=1;
        player = setInterval(function() {
            if(i<4){
                $('#text-con .text:nth-child('+i+')').addClass("active").siblings().removeClass("active");
                $('#cases4 li:nth-child('+i+')').addClass("active").siblings().removeClass("active");
                console.log(i);
            }else{
                i=0;
            }
            i++;
        }, 3000);
    }
    autoPlay();

    var player1;
    $(".column li").mouseenter(
        function(){
            clearInterval(player1);
            var id = $(this).children("a").attr("href");
            $(this).addClass("active").siblings().removeClass("active");
            $(id).addClass("active").siblings().removeClass("active");
        }
    ).mouseleave(
        function(){
