        player = setInterval(function() {
            if(i<4){
                $('#ul-li li:nth-child('+i+')').addClass("active").siblings().removeClass("active");
                $('#jm-ul ul:nth-child('+(i+1)+')').addClass("active").siblings().removeClass("active");
                console.log(i);
            }else{
                i=0;
            }
            i++;
        }, 3000);
    }
    autoPlay();
</script>
</body>
