            return;
        }
        if ($("input[name='phone']").val() === ''){
            alert("手机号不能为空");
            $("input[name='phone']").focus();
            return;
        }
        if ($("input[name='entry_name']").val() === ''){
            alert("项目名不能为空");
            $("input[name='entry_name']").focus();
            return;
        }

        $.post("/plus/web.php", data, function(res){
            console.log(res);
            alert(res.info);
            if(res.status === 1){
                window.location.reload();
            }
        });

    }
</script>
</body>
</html>